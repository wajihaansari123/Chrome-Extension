{
    "name":"color picker",
    "description":"A color Picker Extension",
    "version":"1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"background.js"
    },
    "action":{
        "default_popup":"popup.html"
    },
    "permissions":[
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page": "options.html",
    "icons":{
        "16":"/images/picker_16.png",
        "32":"/images/picker_32.png",
        "48":"/images/picker_48.png"
    }
}